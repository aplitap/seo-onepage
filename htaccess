<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /

RewriteCond %{REQUEST_URI} !^/seo-onepage/public/
RewriteCond %{REQUEST_URI} !^/seo-onepage/api/
RewriteRule ^(.*)$ seo-onepage/index.php [L]

</IfModule>
