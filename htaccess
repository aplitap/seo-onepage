Options -Multiviews

RewriteEngine On
RewriteBase /

# Force search engines to use http://localhost:90/seo-onepage
RewriteCond %{HTTP_HOST} !^http://localhost:90/seo-onepage$
RewriteRule ^(.*) http://http://localhost:90/seo-onepage/$1 [R=301,L]

# Specify search friendly URLs
RewriteRule ^contact$ /index.php?page=contact [L]

